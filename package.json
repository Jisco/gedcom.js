{
  "name": "gedcom.json",
  "version": "1.0.8",
  "description": "Convert GEDCOM Files to an JSON File or an JavaScript-Object and vice versa",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc --build ./tsconfig.json",
    "clean": "tsc --build ./tsconfig.json --clean",
    "demo:JSON": "ts-node src/console.ts --path 'examples/simpsons.ged' --mode JSON",
    "demoFile:JSON": "ts-node src/console.ts --path 'examples/simpsons.ged' --out 'test.json' --mode JSON",
    "demoFileWithProgress:JSON": "ts-node src/console.ts --path 'examples/royal92.ged' --out 'test.json' --showProgress --mode JSON",
    "demo:GEDCOM": "ts-node src/console.ts --path 'examples/simpsons.json' --mode GEDCOM",
    "demoFile:GEDCOM": "ts-node src/console.ts --path 'examples/simpsons.json' --out 'test.ged' --mode GEDCOM",
    "demoFileWithProgress:GEDCOM": "ts-node src/console.ts --path 'examples/royal92.json' --out 'test.ged' --showProgress --mode GEDCOM",
    "test": "nyc mocha",
    "test:watch": "mocha --watch --watch-files src, tests/**/*.ts",
    "doc": "typedoc --out docs src",
    "npm-publish": "npm run build && np",
    "prettier-format": "prettier src/**/*.ts --write",
    "lint": "eslint src --ext .ts",
    "lint:fix": "npm run lint -- --fix"
  },
  "keywords": [
    "gedcom",
    "genealogy",
    "parser",
    "family tree",
    "family",
    "visual family tree",
    "json",
    "ged"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jisco/gedcom.json.git"
  },
  "author": "Jens Hoernecke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jisco/gedcom.json/issues"
  },
  "homepage": "https://github.com/Jisco/gedcom.json#readme",
  "devDependencies": {
    "@types/chai": "^4.2.14",
    "@types/js-yaml": "^3.12.5",
    "@types/lodash": "^4.14.165",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.12",
    "@types/object-path": "^0.11.0",
    "@types/yargs": "^15.0.12",
    "@typescript-eslint/eslint-plugin": "^5.56.0",
    "@typescript-eslint/parser": "^5.56.0",
    "chai": "^4.2.0",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "prettier": "^2.8.7",
    "source-map-support": "^0.5.19",
    "ts-node": "^10.8.1",
    "typescript": "^4.1.3"
  },
  "dependencies": {
    "@hebcal/core": "^3.50.4",
    "dayjs": "^1.11.7",
    "deepdash": "^5.3.9",
    "fclone": "^1.0.11",
    "glob-parent": ">=6.0.2",
    "js-yaml": "^3.14.1",
    "julian-gregorian": "^1.0.0",
    "line-by-line": "^0.1.6",
    "lodash": "^4.17.21",
    "object-path": "^0.11.8",
    "yargs": "^16.2.0"
  }
}
